{
  "name": "react-compose-components",
  "version": "1.0.0",
  "description": "A utility to flatten component pyramids in React.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prepack": "npm run build",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "posttest": "npm run lint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knpwrs/react-compose-components.git"
  },
  "keywords": [
    "react",
    "compose",
    "components",
    "pyramid"
  ],
  "author": "Kenneth Powers <ken@kenpowers.net> (https://knpw.rs)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knpwrs/react-compose-components/issues"
  },
  "homepage": "https://github.com/knpwrs/react-compose-components#readme",
  "peerDependencies": {
    "react": "^16.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^24.0.6",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.0.5",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.3.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^1.2.0",
    "jest": "^24.1.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-testing-library": "^6.0.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  }
}
